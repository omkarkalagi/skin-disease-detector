<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>

  <!-- Skin Condition Visualization -->
  <g transform="translate(200, 150)">
    <!-- Normal Skin -->
    <ellipse cx="-80" cy="0" rx="50" ry="30" fill="#fdbcb4" stroke="#f7a399" stroke-width="1"/>

    <!-- Acne -->
    <ellipse cx="0" cy="0" rx="50" ry="30" fill="#ff9a8b" stroke="#ff6b6b" stroke-width="1"/>
    <circle cx="-15" cy="-5" r="4" fill="#ff6b6b"/>
    <circle cx="10" cy="8" r="3" fill="#ff6b6b"/>
    <circle cx="20" cy="-3" r="2" fill="#ff6b6b"/>

    <!-- Eczema -->
    <ellipse cx="80" cy="0" rx="50" ry="30" fill="#ffcc99" stroke="#ffa366" stroke-width="1"/>
    <path d="M60 -10 Q80 -5 90 -15 Q85 -20 75 -15 Q70 -10 60 -10" fill="#ff8c42" opacity="0.7"/>
  </g>

  <!-- Medical Chart -->
  <rect x="50" y="200" width="300" height="60" rx="4" fill="white" stroke="#e2e8f0" stroke-width="1"/>

  <!-- Chart Bars -->
  <rect x="70" y="220" width="15" height="25" rx="2" fill="#667eea"/>
  <rect x="95" y="215" width="15" height="30" rx="2" fill="#764ba2"/>
  <rect x="120" y="225" width="15" height="20" rx="2" fill="#48bb78"/>
  <rect x="145" y="210" width="15" height="35" rx="2" fill="#f56565"/>

  <!-- Legend -->
  <g transform="translate(200, 240)">
    <circle cx="0" cy="0" r="3" fill="#667eea"/>
    <text x="10" y="3" font-family="Arial, sans-serif" font-size="8" fill="#2d3748">Acne</text>

    <circle cx="50" cy="0" r="3" fill="#764ba2"/>
    <text x="60" y="3" font-family="Arial, sans-serif" font-size="8" fill="#2d3748">Eczema</text>

    <circle cx="100" cy="0" r="3" fill="#48bb78"/>
    <text x="110" y="3" font-family="Arial, sans-serif" font-size="8" fill="#2d3748">Psoriasis</text>

    <circle cx="150" cy="0" r="3" fill="#f56565"/>
    <text x="160" y="3" font-family="Arial, sans-serif" font-size="8" fill="#2d3748">Melanoma</text>
  </g>
</svg>
