<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>

  <!-- Skincare Products -->
  <g transform="translate(80, 100)">
    <!-- Bottle 1 -->
    <rect x="0" y="0" width="30" height="60" rx="4" fill="#667eea"/>
    <rect x="5" y="5" width="20" height="50" rx="2" fill="rgba(255,255,255,0.3)"/>
    <circle cx="15" cy="35" r="8" fill="rgba(255,255,255,0.2)"/>
  </g>

  <g transform="translate(140, 90)">
    <!-- Jar -->
    <ellipse cx="20" cy="15" rx="25" ry="8" fill="#764ba2"/>
    <rect x="0" y="15" width="40" height="45" rx="6" fill="rgba(255,255,255,0.9)"/>
    <ellipse cx="20" cy="23" rx="20" ry="5" fill="#764ba2"/>
  </g>

  <g transform="translate(220, 100)">
    <!-- Tube -->
    <rect x="0" y="0" width="25" height="50" rx="12" fill="#48bb78"/>
    <ellipse cx="12.5" cy="8" rx="12" ry="4" fill="#38a169"/>
    <rect x="5" y="50" width="15" height="10" rx="2" fill="#38a169"/>
  </g>

  <!-- Seasonal Icons -->
  <g transform="translate(320, 80)">
    <!-- Sun -->
    <circle cx="20" cy="20" r="15" fill="#f6ad55"/>
    <g stroke="#dd6b20" stroke-width="2" stroke-linecap="round">
      <line x1="20" y1="5" x2="20" y2="10"/>
      <line x1="35" y1="20" x2="30" y2="20"/>
      <line x1="20" y1="35" x2="20" y2="30"/>
      <line x1="5" y1="20" x2="10" y2="20"/>
      <line x1="10.7" y1="10.7" x2="13.3" y2="13.3"/>
      <line x1="29.3" y1="29.3" x2="26.7" y2="26.7"/>
      <line x1="29.3" y1="10.7" x2="26.7" y2="13.3"/>
      <line x1="10.7" y1="29.3" x2="13.3" y2="26.7"/>
    </g>
  </g>

  <!-- Calendar -->
  <rect x="50" y="200" width="100" height="60" rx="4" fill="white" stroke="#e2e8f0" stroke-width="1"/>
  <rect x="55" y="210" width="15" height="10" rx="1" fill="#667eea"/>
  <text x="62.5" y="218" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">MON</text>

  <!-- Routine Checklist -->
  <g transform="translate(200, 200)">
    <rect x="0" y="0" width="16" height="16" rx="2" fill="#48bb78" stroke="#38a169" stroke-width="1"/>
    <path d="M4 8L6 10L12 4" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="25" y="12" font-family="Arial, sans-serif" font-size="10" fill="#2d3748">Morning Routine</text>
  </g>

  <g transform="translate(200, 225)">
    <rect x="0" y="0" width="16" height="16" rx="2" fill="#48bb78" stroke="#38a169" stroke-width="1"/>
    <path d="M4 8L6 10L12 4" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    <text x="25" y="12" font-family="Arial, sans-serif" font-size="10" fill="#2d3748">Evening Routine</text>
  </g>
</svg>
