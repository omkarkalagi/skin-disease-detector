<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>

  <!-- Medical Form -->
  <rect x="50" y="50" width="300" height="200" rx="8" fill="white" stroke="#e2e8f0" stroke-width="2"/>

  <!-- Form Steps -->
  <circle cx="100" cy="80" r="20" fill="#667eea"/>
  <text x="100" y="87" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">1</text>

  <circle cx="200" cy="80" r="20" fill="#e2e8f0"/>
  <text x="200" y="87" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#667eea">2</text>

  <circle cx="300" cy="80" r="20" fill="#e2e8f0"/>
  <text x="300" y="87" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#667eea">3</text>

  <!-- Checkboxes -->
  <g transform="translate(70, 120)">
    <rect x="0" y="0" width="16" height="16" rx="2" fill="#48bb78" stroke="#38a169" stroke-width="1"/>
    <path d="M4 8L6 10L12 4" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
  </g>

  <g transform="translate(70, 150)">
    <rect x="0" y="0" width="16" height="16" rx="2" fill="#e2e8f0" stroke="#cbd5e0" stroke-width="1"/>
  </g>

  <g transform="translate(70, 180)">
    <rect x="0" y="0" width="16" height="16" rx="2" fill="#e2e8f0" stroke="#cbd5e0" stroke-width="1"/>
  </g>

  <!-- Text Labels -->
  <text x="95" y="135" font-family="Arial, sans-serif" font-size="11" fill="#2d3748">Itching or burning</text>
  <text x="95" y="165" font-family="Arial, sans-serif" font-size="11" fill="#2d3748">Redness or rash</text>
  <text x="95" y="195" font-family="Arial, sans-serif" font-size="11" fill="#2d3748">Swelling or inflammation</text>

  <!-- Progress Bar -->
  <rect x="100" y="220" width="200" height="6" rx="3" fill="#e2e8f0"/>
  <rect x="100" y="220" width="120" height="6" rx="3" fill="#667eea"/>

  <!-- Results Card -->
  <rect x="320" y="120" width="60" height="80" rx="4" fill="#48bb78" opacity="0.8"/>
  <text x="350" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">85%</text>
  <text x="350" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="white">Match</text>
</svg>
